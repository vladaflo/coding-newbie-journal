let numbers = [1, 5, 2, 3, 7, 4, 6, 9, 8]
let sortClosure = numbers.sorted { (a: Int, b: Int) -> Bool in
    return a < b
}
print ("Ascending \(sortClosure)") // result [1, 2, 3, 4, 5, 6, 7, 8, 9]


let sortClosureTwo = numbers.sorted { (a: Int, b: Int) -> Bool in
    return a > b
}
print ("Descending \(sortClosureTwo)") // result [9, 8, 7, 6, 5, 4, 3, 2, 1]
